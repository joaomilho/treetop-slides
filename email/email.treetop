grammar Email
  rule email
    user at domain dot compl dot? compl? 
  end
  rule user
    word
  end
  rule at
    "@"
  end
  rule domain
    word
  end
  rule word
    [\w]+
  end
  rule dot
    "."
  end
  rule compl
    type / country 
  end
  rule type
    "com" / "net" / "org" / "info" / "museum"
  end
  rule country
    "br" / "ar" / "uy" / "us"
  end
end